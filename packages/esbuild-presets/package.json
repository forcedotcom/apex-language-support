{
  "name": "@salesforce/esbuild-presets",
  "version": "0.1.0",
  "description": "Shared esbuild presets and helpers for Node, browser, and worker bundles",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "compile": "tsc --build",
    "clean": "rimraf dist tsconfig.tsbuildinfo"
  },
  "license": "BSD-3-Clause",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "esbuild": ">=0.24.0"
  },
  "devDependencies": {
    "rimraf": "^5.0.5",
    "typescript": "^5.8.2"
  }
}
